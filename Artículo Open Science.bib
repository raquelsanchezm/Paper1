@article{copestakeWellbeingDevelopmentPeru2009,
  title = {Wellbeing and {{Development}} in {{Peru}}: {{Global}} and {{Local Views Confronted}}},
  shorttitle = {Wellbeing and {{Development}} in {{Peru}}},
  author = {Copestake, James},
  year = {2009},
  month = may,
  abstract = {Taking Peru as a case study, the paper examines how contrasting global designs of development can be interrogated using local data on subjective wellbeing. Four examples of the former are presented in the form of shared mental models of Peru as a welfare regime (Section 2). These are then contrasted with a local model -based on data collected from individuals living in seven sites across Central Peru -of subjective wellbeing, which is defined as the gap between individual life goals and their achievement (Section 3). This model is first used to analyse determinants and outcomes of internal migration (Section 4). Congruence and disjuncture between the global and local models are then systematically explored (Section 5). The overall argument is also linked to the idea of "border thinking" advanced by Walter Mignolo (2000). I conclude that empirical research into subjective wellbeing helps to expose the degree of universality as well as gaps and tensions between different perceptions of development. It can also contribute to more reflexive and inclusive analysis of social change and public policy.},
  file = {C\:\\Users\\Raquel\\Zotero\\storage\\Q2YTHB77\\Copestake - 2009 - Wellbeing and Development in Peru Global and Loca.pdf}
}

